.container
	nav.navbar.navbar-inverse
		.navbar-header
			button.navbar-toggle.collapsed(type="button",data-toggle="collapse",data-target="#navbar",aria-expanded="false")
				span.sr-only Toggle navigation
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href="/") MeanListApp
		.navbar-collapse.collapse#navbar
			ul.nav.navbar-nav
				case user
					when false
						li
							a(href="/login") Login
						li
							a(href="/register") Register
					default
						li.navbar-text Hello 
							span= user.name
						li
							a(href="/") My Lists
						li
							a(href="/logout") Logout